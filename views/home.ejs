<%- include("partials/header"); -%>
    <h1>Home</h1>
  <%  usersWithPosts.forEach(function(user){ %>
    <% user.posts.forEach(function(post) { %>
        <h3><%=post.title%></h3>
        <%=post.content.substring(0, 100) + " ..."%>
        <p><a href="/posts/<%=post.title%>">Read More</a> by <%=user.userName%></p>
        </p>
    <% }) %>
  <% }) %>


<%- include("partials/footer"); -%>